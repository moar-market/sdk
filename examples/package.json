{
  "name": "@moar-market/examples",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "Examples showcasing Moar Market SDK usage",
  "author": "Moar Market",
  "license": "MIT",
  "keywords": ["moar", "market", "sdk", "example", "defi", "aptos"],
  "scripts": {
    "start": "tsx src/index.ts",
